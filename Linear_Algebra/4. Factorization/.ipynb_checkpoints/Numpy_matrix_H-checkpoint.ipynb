   "source": [
    "Returns the (complex) conjugate transpose of self.\n",
    "\n",
    "Equivalent to np.transpose(self) if self is real-valued."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "73aab3f6",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "ae7298bb",
   "metadata": {},
   "outputs": [],
   "source": [
    "x = np.matrix(np.arange(12).reshape(3,4))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "a2c8b975",
   "metadata": {},
   "outputs": [
    {
