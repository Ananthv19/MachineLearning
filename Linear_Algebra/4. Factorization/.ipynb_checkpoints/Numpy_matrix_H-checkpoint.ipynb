   "id": "a57a8f93",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "matrix([[ 0. +0.j,  1. -1.j,  2. -2.j,  3. -3.j],\n",
       "        [ 4. -4.j,  5. -5.j,  6. -6.j,  7. -7.j],\n",
       "        [ 8. -8.j,  9. -9.j, 10.-10.j, 11.-11.j]])"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "z = x - 1j*x;\n",
    "z"
   ]
  },
